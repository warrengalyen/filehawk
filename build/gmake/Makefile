# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  demo_config = debug
  filehawk_config = debug
endif
ifeq ($(config),release)
  demo_config = release
  filehawk_config = release
endif

PROJECTS := demo filehawk

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

demo:
ifneq (,$(demo_config))
	@echo "==== Building demo ($(demo_config)) ===="
	@${MAKE} --no-print-directory -C . -f demo.make config=$(demo_config)
endif

filehawk:
ifneq (,$(filehawk_config))
	@echo "==== Building filehawk ($(filehawk_config)) ===="
	@${MAKE} --no-print-directory -C . -f filehawk.make config=$(filehawk_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f demo.make clean
	@${MAKE} --no-print-directory -C . -f filehawk.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   demo"
	@echo "   filehawk"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"